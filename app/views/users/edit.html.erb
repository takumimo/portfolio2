プロフィール編集画面

<%= form_with(model: @user, url: user_path(@user)) do |f| %>
	<h4>名前</h4>
	<%= f.text_field :name, autofocus: true %><br>

	<h4>プロフィール画像</h4>
	<%= f.attachment_field :profile_image, placeholder: "プロフィール画像", class: "user-edit-form" %>

	<h4>自己紹介</h4>
	<%= f.text_area :introduction %><br>

	<h4>メールアドレス</h4>
	<%= f.text_field :email %><br>

	<%= f.submit "更新する" %>
<% end %>






















